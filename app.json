{
  "expo": {
    "name": "WolfKey",
    "slug": "wolfkey-mobile",
    "version": "1.0.10",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "transparent"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.hugoc1000.wolfkeymobile",
      "buildNumber": "10",
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"],
         "ITSAppUsesNonExemptEncryption": false
      },
      "entitlements": {
        "aps-environment": "production"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "useNextNotificationsApi": true,
      "permissions": [
        "com.google.android.c2dm.permission.RECEIVE",
        "android.permission.RECORD_AUDIO"
      ],
      "notification": {
        "icon": "./assets/icon.png",
        "color": "#2563eb",
        "channelId": "default"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "androidStatusBar": {
      "backgroundColor": "transparent",
      "translucent": true
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "We use your camera and photo library to let you take and upload profile photos and share images in posts. For example, you can take a photo to set as your profile picture or attach an image to a discussion post.",
          "cameraPermission": "We use your camera to let you take and upload profile photos and share images in posts. For example, you can take a photo to set as your profile picture or attach an image to a discussion post."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "e33a2ce5-4211-437c-8175-c313f7d6a90c"
      }
    },
    "owner": "hugoc1000"
  }
}
