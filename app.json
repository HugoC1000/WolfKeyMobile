{
  "expo": {
    "name": "WolfKeyMobile",
    "slug": "wolfkey-mobile",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "transparent"
    },
    "ios": {
      "bundleIdentifier": "com.hugoc1000.wolfkeymobile",
      "buildNumber": "2",
      "supportsTablet": true,
      "infoPlist": {
        "NSUserTrackingUsageDescription": "This identifier is used to deliver relevant notifications.",
        "UIBackgroundModes": [
          "remote-notification",
          "remote-notification"
        ],
        "FirebaseAppDelegateProxyEnabled": false,
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "useNextNotificationsApi": true,
      "permissions": [
        "com.google.android.c2dm.permission.RECEIVE",
        "android.permission.RECORD_AUDIO"
      ],
      "notification": {
        "icon": "./assets/icon.png",
        "color": "#2563eb",
        "channelId": "default"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "androidStatusBar": {
      "backgroundColor": "transparent",
      "translucent": true
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your community."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "e33a2ce5-4211-437c-8175-c313f7d6a90c"
      }
    },
    "owner": "hugoc1000"
  }
}
